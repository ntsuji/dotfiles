zshrcDir="$(dirname "$(readlink "${HOME}/.zshrc")")"

fpath=(${zshrcDir}/zsh-completions/src $fpath)

ZSH="${zshrcDir}/oh-my-zsh"
ZSH_THEME='robbyrussell'
plugins=(git z history-substring-search)

source "${ZSH}/oh-my-zsh.sh"
source "${zshrcDir}/zsh-syntax-highlighting/zsh-syntax-highlighting.zsh"

export LANG='ja_JP.UTF-8'
